{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-quotes": 0,
    "no-confusing-arrow": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "no-console": 0,
    "arrow-parens": 0,
  },
  "env": {
    browser: true,
  }
}
